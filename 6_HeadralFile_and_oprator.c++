// Operator in C++:
#include<iostream>
using namespace std;

int main(){
        int a=23,b=12;
        cout<<"The value of of a+b is: "<<a+b<<endl;
        cout<<"The value of of a-b is: "<<a-b<<endl;
        cout<<"The value of of a*b is: "<<a*b<<endl;
        cout<<"The value of of a/b is: "<<a/b<<endl;
        cout<<"The value of of a%b is: "<<a%b<<endl;
}